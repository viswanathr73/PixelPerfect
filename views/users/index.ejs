    <script>
      //Use event delegation to handle clicks on "Add To Cart" buttons
      document
        .querySelector(".product-grid-4")
        .addEventListener("click", async (e) => {
          if (e.target && e.target.classList.contains("add-to-cart-btn")) {
            // Get the product ID from the clicked button's href
            const href = e.target.getAttribute("href");
            const productId = new URLSearchParams(href.split("?")[1]).get("id");

            // Use SweetAlert2 to show a success message for the clicked product
            await Swal.fire({
              title: "Add To Cart",
              text: `You added to the cart!`,
              icon: "success",
              timer: 2000,
              showConfirmButton: false, // Hide the confirmation button
            });

            // Allow the default link navigation
            return true;
          }
        });
    </script>
    <script>
      function addToCart(id) {
        let cartCountValue = parseInt($("#cart-Count").text());
        cartCountValue += 1;

        // Set the incremented value back to the span
        $("#cart-Count").text(cartCountValue);

        $.ajax({
          url: `/addToCart?id=${id}`,
          method: "get",
          success: (response) => {
            if (response.status) {
              Swal.fire({
                title: "ADD TO CART",
                text: "Product Added to cart",
                icon: "success",
                timer: 1000,
                showConfirmButton: false, // This removes the button
              });
            } else {
              location.href = '/login'
            }
          },
        });
      }
    </script>

    <script>
      function wishList(id) {
        $.ajax({
          url: `/addToList?id=${id}`,
          method: "get",
          success: (response) => {
            if (response.status) {
              Swal.fire({
                title: "ADDED TO WISHLIST",
                text: "You seleted a Product to the wishlist",
                icon: "success",
                timer: 3000,
              });
            } else {
              Swal.fire({
                title: "ALREADY SELECTED!",
                text: "You already selected",
                icon: "error",
                timer: 3000,
              });
            }
          },
        });
      }
    </script>