                                                                                                                                %>
                                                                                                                                <a href="/return?id=<%= orders[i].product[j]._id %>&orderId=<%= orders[i]._id %>"
                                                                                                                                    class="
                                                                                                                                        btn
                                                                                                                                        btn-md
                                                                                                                                        rounded
                                                                                                                                        font-sm">Return</a>
                                                                                                                                <% } else
                                                                                                                                    if(orders[i].status=='returned'
                                                                                                                                    ){
                                                                                                                                    %>

                                                                                                                                    <% }
                                                                                                                                        %>


                                                                                                        </td>
                                                                        </tr>

                                                                        <% } %>
                                                                            <% } %>
                                                                                <% } else { %>
                                                                                    <tr>
                                                                                        <td colspan="2">No item found in
                                                                                            Oders</td>
                                                                                    </tr>
                                                                                    <% } %>


                                                        </tbody>
                                                    </table>

                                                </div> <!-- table-responsive //end -->
